{
  "name": "nuxt-auth-demo-app",
  "private": true,
  "type": "module",
  "version": "0.2.3",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build-check": "nuxt build",
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest --dir tests/unit --passWithNoTests --coverage",
    "test:unit": "vitest --dir tests/unit --passWithNoTests",
    "test:unit:cov": "vitest --dir tests/unit --coverage",
    "test:e2e": "npx playwright test --pass-with-no-tests",
    "test:e2e:ui": "npx playwright test --ui --pass-with-no-tests"
  },
  "dependencies": {
    "@sbc-connect/nuxt-auth": "workspace:*"
  },
  "devDependencies": {
    "@sbc-connect/eslint-config": "workspace:*",
    "@sbc-connect/playwright-config": "workspace:*",
    "@sbc-connect/vitest-config": "workspace:*",
    "nuxt": "catalog:",
    "typescript": "catalog:",
    "vue-tsc": "catalog:"
  }
}
